<!DOCTYPE log>
<log>
	<id>urn:com:ebc:neo4j:server:log</id>
	<name>Neo4j Server Log</name>
	<handlers>
		<handler>
			<instance>
				<class>org.netkernel.layer0.logging.LogFileHandler</class>
				<string>N:/log/neo4j_server.%g.log</string>
				<int>50000000</int>
				<int>3</int>
				<boolean>true</boolean>
			</instance>
			<formatterClass>org.netkernel.layer0.logging.ConsoleFormatter</formatterClass>
		</handler>
	</handlers>
</log>