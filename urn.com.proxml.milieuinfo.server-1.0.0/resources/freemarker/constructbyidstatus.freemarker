<#setting url_escaping_charset='UTF-8'>
CONSTRUCT {
  <http://id.milieuinfo.be/${owner}/${id}/${status?url}#id> ?predicate ?object .
  <http://id.milieuinfo.be/${owner}/${id}/${status?url}#id> ?outgoingpredicate ?outgoingobject .
  ?outgoingobject <http://www.w3.org/2000/01/rdf-schema#label> ?outgoinglabel .
  ?incomingsubject ?incomingpredicate <http://id.milieuinfo.be/${owner}/${id}/${status?url}#id> .
  <#--
  ?incomingsubject <http://www.w3.org/2000/01/rdf-schema#label> ?incominglabel .
  -->
}
WHERE {
  SELECT *
  WHERE {
    OPTIONAL {
      <http://id.milieuinfo.be/${owner}/${id}/${status?url}#id> ?predicate ?object .
      FILTER (isLiteral(?object))
    }
    OPTIONAL {
      <http://id.milieuinfo.be/${owner}/${id}/${status?url}#id> ?outgoingpredicate ?outgoingobject .
      FILTER (isURI(?outgoingobject))
    }
    OPTIONAL {
      ?outgoingobject <http://www.w3.org/2000/01/rdf-schema#label> ?outgoinglabel .
    }
    OPTIONAL {
      ?incomingsubject ?incomingpredicate <http://id.milieuinfo.be/${owner}/${id}/${status?url}#id> .
    }
    <#--
    OPTIONAL {
      ?incomingsubject <http://www.w3.org/2000/01/rdf-schema#label> ?incominglabel .
    }
    -->
  }
}